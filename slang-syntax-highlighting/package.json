{
    "name": "slang-syntax-highlighting",
    "displayName": "Slang syntax highlighting",
    "description": "Highlights syntax in .slang files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "slang",
            "aliases": ["slang", "slang"],
            "extensions": [".slang"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "slang",
            "scopeName": "source.slang",
            "path": "./syntaxes/slang.tmLanguage.json"
        }]
    }
}